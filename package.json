{
  "name": "butchbot",
  "version": "0.0.2",
  "description": "A dice roller and character manager bot for playing Thirsty Sword Lesbians in Discord",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && prisma generate",
    "lint": "npx prettier --write .",
    "start": "npm run serve",
    "serve": "node dist/index.js",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "keywords": [
    "discord",
    "bot",
    "thirsty sword lesbians",
    "tsl"
  ],
  "author": "Cade Richard <cade@caderichard.com>",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "prisma": "^2.21.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@prisma/client": "^2.21.2",
    "@types/express": "^4.17.11",
    "body-parser": "1.19.0",
    "eris": "^0.15.0",
    "express": "^4.17.1",
    "rpg-dice-roller": "^5.0.0"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.3"
  },
  "engines": {
    "node": "^14.0.0"
  }
}
